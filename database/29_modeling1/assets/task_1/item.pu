@startuml

package 商品マスタ {
    entity 単品商品 {
        * 単品商品ID
        --
        商品名
        商品種別ID [FK]
    }

    entity 単品商品種別 {
        * 商品種別ID
        --
        単品商品種別名
        単価
    }

    entity セット商品 {
        * セット商品ID
        --
        商品名
        単価
    }

    entity セット商品種別 {
        * セット商品種別ID
        --
        セット商品種別名
    }

    単品商品 |o--|| 単品商品種別
    セット商品 |o--|| セット商品種別
}

@enduml